class player:
	def __init__(self,data):
		self.strength = data[0]
		self.intel = data[1]
		self.charis = data[2]
		self.stamina = data[3]
		self.health = data[4]
		self.defence = data[5]
		self.agility = data[6]
		self.AT = data[7]
		self.weapon = None
		self.Armour = None
		self.drink = []
		self.food = []
		self.other = []
